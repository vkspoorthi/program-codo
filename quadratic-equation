#include<stdio.h>

#include<math.h>
void input (double a,double b,double c) 
{
compute (a,b,c)
}
void compute(double a1,double b1, double c1)

{

double determinant=b1*b1-4*a1*c1;
output(determinant);

}

void output(double determinant1)

{

double root1,root2,realPart,imaginaryPart;
if(determinant1>0)

{
 root1 = (-b+sqrt(determinant1))/(2*a);
        
 root2 = (-b-sqrt(determinant1))/(2*a);


        
printf("root1 = %.2lf and root2 = %.2lf",root1 , root2);
 
}

 else if(determinant1 ==0)
{ 
 
root1 = root2 = -b/(2*a);

 
printf("root1 = root2 = %.2lf;", root1);

 }

else
{
 realPart = -b/(2*a);
        
imaginaryPart = sqrt(-determinant)/(2*a);
        
printf("root1 = %.2lf+%.2lfi and root2 = %.2f-%.2fi", realPart, imaginaryPart, realPart, imaginaryPart);
      
}


void main()
{
double x=34,y=20,z=12;
intput(x,y,z);
}
